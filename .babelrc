{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
      }
    ],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ],
    [
      "styled-jsx/babel", {
        "plugins": [
          ["style-jsx-plugin-postcss", {"grid": true}]
        ]
      }
    ]
  ],
  "presets": ["next/babel"],
  "ignore": [],
  "env": {
    "test": {
      "presets": [["env", { "modules": "commonjs" }], "next/babel"]
    }
  }
}
